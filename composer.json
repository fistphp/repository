{
  "name": "fist/repository",
  "description": "The Fistlab PHP Repository Components",
  "keywords": ["fistlab", "fistphp", "component", "repository"],
  "license": "MIT",
  "homepage": "https://github.com/fistphp/repository",
  "support": {
    "issues": "https://github.com/fistlab/php/issues",
    "source": "https://github.com/fistlab/php"
  },
  "authors": [
    {
      "name": "Mark Topper",
      "email": "mark@ulties.com"
    }
  ],
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4",
    "fist/testing": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Fist\\Repository\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "suggest": {
    "fist/container": "Required for using ContainerRepository"
  },
  "minimum-stability": "dev"
}
